package test;

import org.junit.jupiter.api.Assertions;
import org.junit.jupiter.api.Test;
import org.mockito.Mockito;

public class UnitTesting {
	
	@Test
	public void testAdd() {
		Calculator calcobj=Mockito.mock(Calculator.class);
		//Calculator calcobj=new Calculator();
		Mockito.when(calcobj.add(Mockito.anyInt(), Mockito.anyInt())).thenCallRealMethod();
		Assertions.assertEquals(40, calcobj.add(20,20));
	}
	
	/*@Test
