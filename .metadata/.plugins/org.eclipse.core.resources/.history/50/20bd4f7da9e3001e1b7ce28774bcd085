package test;

public class assignment {
	private emailService es;
	
	public forgotPassword(emailService es) {
		this.es=es;
	}
	
	public boolean resetPassword(String email) {
		if(isValidEmail(email)) {
			es.resent(email);
			return true;
		}
		return false;
	}
	
	private boolean isValidEmail(String email) {
		return true;
	}
	
}


